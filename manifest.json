{
    "manifest_version": 2,
    "name": "CF Problem Finder",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    }
  }